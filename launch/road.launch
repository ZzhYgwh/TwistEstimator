
<launch>
 
  <!-- 启动 test_detector2.py -->
  <node pkg="twist_estimator" type="twist_estimator_node" name="twist_estimator_node" output="screen">
    <param name="config_file" value="$(find twist_estimator)/config/road.yaml"/>
  </node>  
  <!-- 启动 combi_radar.py -->
  <node pkg="twist_estimator" type="combi_radar_road.py" name="combi_radar" output="screen"/>

  <!-- 启动 catch_radar_twist_road.py -->
  <node pkg="twist_estimator" type="catch_radar_twist_road.py" name="catch_radar_twist" output="screen"/>

  <!-- 启动 catch_radar_estimator_road.py -->
  <node pkg="twist_estimator" type="catch_radar_estimator_road.py" name="catch_radar_estimator" output="screen"/>

  <!-- 启动 catch_gt_twist_road.py -->
  <node pkg="twist_estimator" type="catch_gt_twist_road.py" name="catch_gt_twist" output="screen"/>

  <!-- 启动 rviz 并加载指定的配置文件 -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find twist_estimator)/config/rviz_config.rviz" output="screen" />
  <node pkg="rviz" type="rviz" name="rviz2" args="-d $(find twist_estimator)/config/rviz_config2.rviz" output="screen" />
  
  <!-- Load URDF file -->
  <!-- <param name="robot_description" command="cat $(find twist_estimator)/launch/model/DJImini22.urdf" /> -->

  <!-- Start robot_state_publisher to publish the robot model -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen">
    <param name="publish_frequency" value="50" />
  </node>

</launch>
